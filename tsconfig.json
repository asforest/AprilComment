{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitThis": false,
        "alwaysStrict": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "Node",
        "target": "ES5",
        "downlevelIteration": true
    }
}